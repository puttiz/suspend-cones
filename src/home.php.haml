/
  Feature
  ============================================================================
.feature
  .carousel.slide.box.light.container
    .carousel-inner
      .item.active
        %img(src="http://placehold.it/900x400")
        %span.tag.dark.lb
          %ol
            %li
              %a(href="#") link 1
            %li
              %a(href="#") link 2 long long long long
        %span.tag.light.rt
          %ul
            %li
              %a(href="#") link 3
            %li
              %a(href="#") link 4 long long long long
    %a.prev.arrow(href="#myCarousel" date-slide="prev") ‹
    %a.next.arrow(href="#myCarousel" date-slide="next") ›
/
  Marketing
  ============================================================================
.marketing
  %ul.thumbnails
    - (0...6).each do
      %li.span4
        .thumbnail.box.light.f
          %img(src="http://placehold.it/284x200")

/ /marketing